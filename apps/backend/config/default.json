{
  "database": {
    "url": null,
    "ssl": false,
    "pool": {
      "min": 2,
      "max": 20,
      "idleTimeoutMillis": 30000,
      "connectionTimeoutMillis": 10000,
      "maxLifetimeSeconds": 600,
      "keepAlive": true,
      "keepAliveInitialDelayMillis": 5000
    }
  },
  "cookie": {
    "secret": null
  },
  "frontendUrl": "http://localhost:5173",
  "hashSaltRounds": 12,
  "logLevel": "debug",
  "server": {
    "host": "0.0.0.0",
    "port": 5000
  },
  "rateLimit": {
    "global": {
      "max": 100,
      "timeWindow": 60000
    },
    "register": {
      "max": 5,
      "timeWindow": 600000
    },
    "login": {
      "max": 10,
      "timeWindow": 600000
    },
    "logout": {
      "max": 20,
      "timeWindow": 60000
    },
    "refreshToken": {
      "max": 30,
      "timeWindow": 60000
    },
    "profile": {
      "max": 50,
      "timeWindow": 60000
    },
    "accountDeletion": {
      "max": 3,
      "timeWindow": 3600000
    }
  },
  "token": {
    "access": {
      "secret": null,
      "expiresIn": 600
    },
    "refresh": {
      "secret": null,
      "expiresIn": 604800,
      "graceMs": 10000,
      "idempotencyMs": 3000
    }
  }
